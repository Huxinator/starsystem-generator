{
  "name": "st-universe/starsystem-generator",
  "autoload": {
    "psr-4": {
      "Stu\\StarsystemGenerator\\": "src/StarsystemGenerator"
    }
  },
  "require": {
    "php": "^7.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.3",
    "mockery/mockery": "^1.1",
    "st-universe/assets": "dev-master",
    "mnsami/composer-custom-directory-installer": "1.1.*"
  },
  "config": {
    "platform": {
      "php": "7.3"
    }
  },
  "scripts": {
    "tests": "@php vendor/bin/phpunit tests"
  },
  "extra": {
    "installer-paths": {
      "./example/assets": [
        "st-universe/assets"
      ]
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:st-universe/assets.git"
    }
  ]
}
